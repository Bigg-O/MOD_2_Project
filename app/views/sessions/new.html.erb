Login
<%= form_with model:@user, url:"/login", method:"post" do |f|%>
    <%= f.label :user_name%>
    <%= f.text_field :user_name%><br>
    <%= f.label :password%>
    <%= f.password_field  :password%><br>
    <%= f.submit"Login" %>
    <% end %>
    <% if @user.errors.any? %>
      <%= @user.errors.full_messages %>
        <% else %>
            <%= @message %>
    <% end %>